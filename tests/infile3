{-# LANGUAGE GADTs #-}

data Boo a b where
  --Foo :: a -> (b -> Boo a b) -> Boo a a
  Foo :: a -> ([c] -> Boo a b) -> Boo a a
  --Foo :: a -> (d -> (Boo a b -> [c]) -> b) -> Boo a a
  --Foo :: a -> (((a -> Boo c d) -> Boo a b -> [c]) -> b) -> Boo a a
  Joe :: a -> Boo (b -> Boo a b) Int -> Boo a a
  Lin :: a -> (b -> Int) -> Boo a b